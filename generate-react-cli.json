{
  "usesTypeScript": true,
  "usesCssModule": false,
  "cssPreprocessor": "css",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "customTemplates": {
        "component": "src/templates/component/TemplateName.tsx"
      },
      "path": "src/components",
      "withStyle": false,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    },
    "ui": {
      "customTemplates": {
        "component": "src/templates/component/TemplateName.tsx"
      },
      "path": "src/components/ui",
      "withStyle": false,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    },
    "view": {
      "customTemplates": {
        "component": "src/templates/component/TemplateName.tsx"
      },
      "path": "src/screens",
      "withStyle": false,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    }
  }
}
